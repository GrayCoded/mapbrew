<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Your Map Library</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="../css/reset.css" />
    <link rel="stylesheet" href="../css/league.css" />
    <link rel="stylesheet" href="../css/style.css" />
    <!-- Google Fonts -->
    <link
      href="https://fonts.googleapis.com/css2?family=Cinzel+Decorative&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Kalam&display=swap"
      rel="stylesheet"
    />
    <!-- Swiper -->
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.css"
    />
  </head>

  <body>
    <div id="everything">
      <div class="row m-0">
        <nav class="navbar">
          <div class="col">
            <!-- column to make mapbrew centered -->
          </div>
          <div class="col d-flex justify-content-center">
            <a href="index">
              <p id="navTitle">Mapbrew</p>
            </a>
          </div>
          <div class="right col d-flex justify-content-end">
            <button class="navButton"><a href="login">Login</a></button>
            <button class="navButton">
              <a href="mapbuilder">Map Builder</a>
            </button>
            <button class="navButton"><a href="league">League</a></button>
            <button class="navButton"><a href="faq">FAQ</a></button>
          </div>
        </nav>
      </div>
      <section id="slider-1">
        <div class="swiper" id="swiper-1">
          <div class="swiper-wrapper">
            <div class="swiper-slide">
              <img src="../images/one.jpeg" alt="map 2" />
            </div>
            <!--end of swiper slide-->
            <div class="swiper-slide">
              <img src="../images/two.jpeg" alt="map 3" />
            </div>
            <!--end of swiper slide-->
            <div class="swiper-slide">
              <img src="../images/three.jpeg" alt="map 4" />
            </div>
            <!--end of swiper slide-->
            <div class="swiper-slide">
              <img src="../images/four.jpeg" alt="map 5" />
            </div>
            <!--end of swiper slide-->
          </div>
          <!--end of swiper wrapper-->
          <div class="swiper-pagination"></div>
        </div>
        <!--end swiper-->
      </section>
    </div>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
      crossorigin="anonymous"
    ></script>
    <script src="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.js"></script>
    <script src="../js/league.js"></script>
  </body>
</html>
